{
  "rules": {
    "no-any": false,
    "no-null-keyword ": false,
    "cyclomatic-complexity": [true, 20],
    "trailing-comma": [
      true,
      {"multiline": "never", "singleline": "never"}
    ],
    "no-console": [
      true,
      "log",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ]
  }
}
